{
  "name": "sisi-blur",
  "displayName": "sisi-blur",
  "version": "0.0.2",
  "description": "Blur images of Abdel Fattah Sisi",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build --target=firefox-mv2 --source-maps",
    "web-ext:sign": "web-ext sign --api-key=$JWT_USERNAME --api-secret=$JWT_SECRET --channel=listed --source-dir=build/firefox-mv2-prod",
    "release:ci:firefox": "pnpm build && pnpm web-ext:sign",
    "cs": "pnpm changeset",
    "csv": "pnpm changeset version"
  },
  "dependencies": {
    "@hono/node-server": "^1.12.2",
    "@plasmohq/messaging": "^0.6.2",
    "@plasmohq/storage": "^1.12.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-switch": "^1.1.0",
    "@vladmandic/human": "^3.3.2",
    "plasmo": "^0.89.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "webext-storage": "^1.2.2"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.8",
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@plasmohq/rps": "^1.8.7",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "autoprefixer": "^10.4.20",
    "clsx": "^2.1.1",
    "postcss": "^8.4.47",
    "prettier": "3.2.4",
    "tailwind-merge": "^2.5.2",
    "tailwindcss": "^3.4.11",
    "typescript": "5.3.3",
    "web-ext": "^8.2.0"
  },
  "manifest": {
    "license": [
      "MIT"
    ],
    "permissions": [
      "activeTab",
      "tabs",
      "storage"
    ],
    "browser_specific_settings": {
      "gecko": {
        "id": "{eefb93d8-fd4a-465e-b6d0-6a4430e51f45}"
      }
    },
    "host_permissions": [
      "https://*/*",
      "http://*/*"
    ],
    "web_accessible_resources": [
      {
        "matches": [
          "<all_urls>"
        ],
        "resources": [
          "samples/",
          "models/"
        ]
      }
    ],
    "externally_connectable": {
      "matches": [
        "*://*.localhost.com/*"
      ]
    }
  },
  "contributors": [
    "louis <louis@plasmo.com>",
    "udayan"
  ]
}